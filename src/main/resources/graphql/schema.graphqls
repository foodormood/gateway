type Query {
    items : [Item],
    categories: [Category]
}

type Mutation {
    createCategory(input: CreateCategoryInput!): Category!
}

type Item {
    id: ID!
    name: String
}

input CreateCategoryInput {
    name: String
    nameEn: String
    order: Int
}

input UpdateCategoryInput {
    id: ID!
    name: String
    nameEn: String
    order: Int
}

type Category {
    id: ID!
    name: String
    nameEn: String
    order: Int
}
